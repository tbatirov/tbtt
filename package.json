    "lucide-react": "^0.344.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.2",
    "react-router-dom": "^6.22.1",
    "tsx": "^4.7.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.1"
